<nodeDefs>
    <!-- Nest Control -->
    <nodeDef id="nestcontrol" nls="nctl">
        <editors />
        <sts />
        <cmds>
            <sends />
            <accepts>
			    <cmd id="DISCOVER" />
		    </accepts>
        </cmds>
    </nodeDef>
	<!-- Nest Thermostat -->
    <nodeDef id="nestdevice" nls="ndev">
        <editors />
        <sts>
            <st id="ST" editor="nmode" />
			<st id="GV1" editor="ntemp" />
            <st id="GV2" editor="ntemp" />
			<st id="GV3" editor="ntemp" />
			<st id="GV4" editor="nstatus" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="TEMP">
                    <p id="" editor="ntemp" optional="T" init="GV3" />
                </cmd	>
                <cmd id="MODE">
                    <p id="" editor="nmode" optional="T" init="ST" />
				</cmd>	
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>